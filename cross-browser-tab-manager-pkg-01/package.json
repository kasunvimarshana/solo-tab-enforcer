{
  "name": "cross-browser-tab-manager",
  "version": "1.0.0",
  "description": "A comprehensive cross-browser tab management solution with single-tab enforcement and observation capabilities",
  "main": "dist/tab-manager.js",
  "module": "dist/tab-manager.esm.js",
  "browser": "dist/tab-manager.umd.js",
  "types": "dist/tab-manager.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node build/build-cjs.js",
    "build:esm": "node build/build-esm.js",
    "build:umd": "node build/build-umd.js",
    "build:types": "node build/build-types.js",
    "test": "node test/test-runner.js",
    "lint": "node build/lint.js",
    "prepublishOnly": "npm run build && npm run test",
    "dev": "node build/dev-server.js"
  },
  "keywords": [
    "tab-management",
    "single-tab",
    "browser-tabs",
    "cross-browser",
    "tab-enforcement",
    "tab-observer",
    "javascript",
    "browser-storage",
    "localStorage",
    "sessionStorage",
    "vanilla-js",
    "no-dependencies"
  ],
  "author": "Kasun Vimarshana",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kasunvimarshana/solo-tab-enforcer.git"
  },
  "bugs": {
    "url": "https://github.com/kasunvimarshana/solo-tab-enforcer/issues"
  },
  "homepage": "https://github.com/kasunvimarshana/solo-tab-enforcer#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie < 11"
  ],
  "devDependencies": {},
  "dependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {}
}